{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "value": "[profile.property.region]"
        },
        "nsg": {
            "value": "[profile.network.settingObject.nsg]"
        },
        "logging": {
            "value": "[profile.network.settingObject.loggingWorkspace]"
        },
        "tags": {
            "value": "[profile.tags]"
        }
    }
}